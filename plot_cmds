set terminal png size 600, 800 enhanced font "Fira Mono,8"

set logscale y 2
set output "bench_plot.png"
plot "bench_data" using 1:2 with impulses title 'Naive',\
     "bench_data" using 1:3 with impulses title 'Vec Sweep',\
     "bench_data" using 1:4 with impulses title 'List Sweep',\
     "bench_data" using 1:5 with impulses title 'AVL Sweep'

set output "bench_sweep_plot.png"
plot "bench_data" using 1:3 with impulses title 'Vec Sweep',\
     "bench_data" using 1:4 with impulses title 'List Sweep',\
     "bench_data" using 1:5 with impulses title 'AVL Sweep'
