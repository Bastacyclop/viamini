set terminal png size 600, 600 enhanced font "Fira Mono,8"

set logscale y 2
set output "plot.png"
set xlabel "fichier"
set ylabel "vias"
plot "data" using 1:4 with impulses title 'Horizontal/Vertical',\
     "data" using ($1+0.15):6 with impulses title 'Cycles impairs'

